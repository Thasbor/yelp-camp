<% layout('layouts/boilerplate') %>
<h1> <%= camp.title %></h1>
<h2> <%= camp.location %></h2>
<p> <%= camp.price %> </p>
<img src="<%= camp.image %>" alt="">
<p><%= camp.descript %></p>
<footer>
    <a href="/campgrounds/<%=camp._id%>/edit"><button>Edit</button></a>
    <a href="/campgrounds"><button>All Campgrounds</button></a>
        
</footer>
<p>
    <form action="/campgrounds/<%=camp._id%>?_method=DELETE" method="POST">
        <button>Delete</button>
    </form>
</p>

    
